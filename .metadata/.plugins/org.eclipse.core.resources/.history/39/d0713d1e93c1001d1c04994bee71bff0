package uguryagmur;

public class Monkey extends Mammals {

	public Monkey(String name, int weight, int length, boolean isDangerous,String className) {
		super(name, weight, length, isDangerous, className);
		setClassName("Mammals");
		setDangerous(false);
	}

	@Override
	public void walk() {
		System.out.println(getName() + " yürüyor!");
		
	}

}
