package uguryagmur;

public class CustomString {
	
	private String word;

	public CustomString() {
		
		this.word = "BoostMaratonSinavi";
	}
	
	public int uzunluk() {
		int toplam=this.word.length();
		
		return toplam;
	}
	
	public String belirtilenIndextekiKarakteriDon(int index) {
		String letter=String.valueOf(word.charAt(index));
		
		return letter;
	}
	
	
	
	
	
	
	

}
