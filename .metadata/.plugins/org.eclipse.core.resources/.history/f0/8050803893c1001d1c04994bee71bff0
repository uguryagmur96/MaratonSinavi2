package uguryagmur;

public class Lizard extends Reptiles {

	public Lizard(String name, int weight, int length, boolean isDangerous,String className) {
		super(name, weight, length, isDangerous, className);
		setClassName("Reptiles");
		setDangerous(false);
	}

	@Override
	public void creep() {
		System.out.println(getName() + " sürünerek gidiyor");
		
	}
	public void roar() {
		System.out.println(getName() + " çok ayrı tıslıyor!");
	}

}
