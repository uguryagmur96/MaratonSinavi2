package uguryagmur;

public abstract class Reptiles extends Animal {
	
	private String className;

	public Reptiles(String name, int weight, int length, boolean isDangerous,String className) {
		super(name, weight, length, isDangerous);
		this.className=className;
		
	}

	@Override
	public void roar() {
		// TODO Auto-generated method stub
		
	}
	
	
	public abstract void creep();
	
	
	

}
