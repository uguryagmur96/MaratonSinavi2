package uguryagmur;

public abstract class Reptiles extends Animal {

	public Reptiles(String name, int weight, int length, boolean isDangerous) {
		super(name, weight, length, isDangerous);
		
	}

	@Override
	public void roar() {
		// TODO Auto-generated method stub
		
	}
	
	
	public abstract void creep();
	
	
	

}
