package uguryagmur;

public class Lion extends Mammals {
	
	private String className;
	
	
	

	public Lion(String name, int weight, int length) {
		super(name, weight, length);
		setDangerous(true);
		setClassName("Mammals");
	}

	@Override
	public void walk() {
		System.out.println(getName()+ " usulca y端r端yor!");
		
	}
	
	public void roar() {
		System.out.println(getName() + " k端kr端yor!");
	}

}
